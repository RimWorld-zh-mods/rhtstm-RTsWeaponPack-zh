     <RecipeDef>
    <defName>FiveSevenCraft</defName>
    <label>Make FN Five Seven</label>
    <description>Craft FN Five Seven.</description>
    <jobString>Making FN Five Seven.</jobString>
	<unfinishedThingDef>UnfinishedWeapon</unfinishedThingDef>
	<workSpeedStat>SmithingSpeed</workSpeedStat>
    <workAmount>32000</workAmount>
	<workSkill>Crafting</workSkill>
    <products>
        <Gun_FiveSeven_rt>1</Gun_FiveSeven_rt>
    </products>
    <ingredients>
		<li>
			<filter>
				<thingDefs>
					<li>Steel</li>
				</thingDefs>
			</filter>
			<count>45</count>
		</li>
		<li>
			<filter>
				<thingDefs>
					<li>Component</li>
				</thingDefs>
			</filter>
			<count>2</count>
		</li>
    </ingredients>
	<fixedIngredientFilter>
         <thingDefs>
            <li>Steel</li>
			<li>Component</li>
         </thingDefs>
    </fixedIngredientFilter>
	<recipeUsers>
		<li>WeaponAssemblyTable_RT</li>
	</recipeUsers>
	<researchPrerequisite>Pistols_RT</researchPrerequisite>
	<skillRequirements>
		<li>
			<skill>Crafting</skill>
			<minLevel>3</minLevel>
		</li>
	</skillRequirements>
  </RecipeDef>


